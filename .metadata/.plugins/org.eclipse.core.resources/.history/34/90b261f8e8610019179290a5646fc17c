//package temp;

import java.io.DataInputStream;
import java.io.DataOutputStream;
import java.net.InetAddress;
import java.net.Socket;
import java.util.Scanner;

public class temp_class {
	public static void main(String [] args) throws Exception {
		Socket client= new Socket("localhost",10002);
		Scanner sc = new Scanner(System.in);
		String str = sc.nextLine();
		DataOutputStream outtoserver = new DataOutputStream(client.getOutputStream());
		DataInputStream infromserver = new DataInputStream(client.getInputStream());
		outtoserver.writeBytes(str);
		outToServer.flush();
		System.out.println("server output: "+infromserver.readUTF());
		outtoserver.close();
		infromserver.close();
	}
}
